programa {
  funcao inicio() {
    inteiro viagens, pessoas, alemanha, portugal, italia
    viagens = 0
    pessoas = 0
    alemanha = 300
    portugal = 200
    italia = 300
    escreva ("Quantas pessoas irão embarcar? ")
    leia (pessoas)
    escreva ("Quantas viajens? ")
    leia (viagens)
    escreva (pessoas * alemanha + portugal + italia)
    leia ()
  }
}
