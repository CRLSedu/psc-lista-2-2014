programa 
{
  funcao inicio()
  {
    inteiro num1, num2
    cadeia soma, subtracao, multiplicacao
    escreva ("Digite o primeiro número: ")
    leia (num1)
    escreva ("Digite o segundo número: ")
    leia (num2)
    escreva ("Soma:\n", num1 + num2)
    leia (soma)
    escreva ("Subtração:\n", num1 - num2)
    leia (subtracao)
    escreva ("Multiplicação:\n", num1 * num2)
    leia (multiplicacao)
  }
}
